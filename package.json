{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "delete": "adb uninstall com.client",
    "st": "watchman watch-del-all && yarn start -- --reset-cache",
    "build:dev": "rm -rf .env && cp .env.dev .env && npm run build",
    "build:qualif": "rm -rf .env && cp .env.qualif .env && npm run build",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "android-qualif": "ENVFILE=.env.qualif react-native run-android",
    "android-qualif-win": "set ENVFILE=.env.qualif && react-native run-android",
    "android-dev-win": "set ENVFILE=.env.dev && react-native run-android",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "web": "react-scripts start",
    "web-dev": "set process.env=.env.dev && react-scripts start",
    "test": "jest",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.12.1",
