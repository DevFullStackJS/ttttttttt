    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@react-pdf/renderer": "^1.6.13",
    "@types/react-datepicker": "^3.1.2",
    "@types/react-native-snap-carousel": "^3.8.2",
    "axios": "^0.21.0",
    "dateformat": "^4.5.1",
    "dayjs": "^1.10.4",
    "localforage": "^1.9.0",
    "qrcode": "^1.4.4",
    "react": "17.0.2",
    "react-alert": "^7.0.2",
    "react-datepicker": "^3.4.1",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.2.4",
    "react-hook-form": "^6.14.1",
    "react-icons": "^4.2.0",
    "react-native": "0.65.1",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-camera": "^3.44.1",
    "react-native-charts-wrapper": "^0.5.7",
    "react-native-config": "^1.4.2",
    "react-native-exit-app": "^1.1.0",
    "react-native-file-viewer": "^2.1.4",
    "react-native-geolocation-service": "^5.3.0-beta.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-google-places-autocomplete": "^2.4.0",
    "react-native-image-picker": "^4.0.6",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-maps": "^0.28.0",
    "react-native-maps-directions": "^1.8.0",
    "react-native-media-query": "^1.0.3",
    "react-native-permissions": "^3.0.4",
    "react-native-pie": "^1.1.2",
    "react-native-qrcode-scanner": "^1.5.4",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-shadow": "^1.2.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
